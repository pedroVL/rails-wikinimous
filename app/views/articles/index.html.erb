<div class="header"><h1>Articles</h1><%= link_to "+ New article", new_article_path %></div>
<div class="list">
  <ul>
    <% @articles.each do |article| %>
    <li>
      <h3><%= link_to article.title, article_path(article) %></h3>
      <p><%= article.content %></p>
    </li>
    <br>
    <% end %>
  </ul>
</div>

